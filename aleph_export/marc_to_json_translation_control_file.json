{
	"root" : {
		"FieldsToExtract": [
			{ "label": "apiVersion",
				"externalProcess": "schema_api_version",
				"format": "text"
			},
			{ "label": "fileCreatedDate",
				"externalProcess": "file_created_date",
				"format": "text"
			},
			{ "label": "sourceSystem",
				"constant": "Aleph",
				"format": "text"
			},
			{ "label": "repository",
				"fields": ["852"],
				"subfields": ["b"],
				"extraProcessing": "translate_repository",
				"format": "text"
			},
			{ "label": "sequence",
				"constant": 1,
				"format": "text"
			},
			{ "label": "title",
				"fields": ["245"],
				"subfields": ["a", "b"],
				"format": "text"
			},
			{ "label": "collectionId",
				"fields": ["001"],
				"format": "text"
			},
			{ "label": "parentId",
				"constant": "root",
				"format": "text"
			},
			{ "label": "id",
				"fields": ["001"],
				"format": "text"
			},
			{
				"label": "uniqueIdentifier",
				"fields": ["852"],
				"subfields": ["h"],
				"specialSubfields": ["i"],
				"ind1": ["0"],
				"extraProcessing": "format_call_number",
				"format": "text"
			},
			{ "label": "creators",
				"fields": ["100", "199"],
				"selection": "range",
				"subfields": ["a", "b", "c", "e", "q"],
				"specialSubfields": ["d"],
				"extraProcessing": "format_creators",
				"format": "array",
				"note": "added e 5/15/20 sm"
			},
			{ "label": "contributors",
				"fields": ["700"],
				"selection": "range",
				"subfields": ["a", "b", "c", "d", "e", "q"],
				"specialSubfields": ["d"],
				"extraProcessing": "format_creators",
				"format": "array",
				"note": "added 5/15/20 sm"
			},
			{ "label": "collections",
				"fields": ["791"],
				"selection": "range",
				"subfields": ["a", "b", "c", "d", "e", "q"],
				"specialSubfields": ["d"],
				"format": "array",
				"note": ["added 710 7/20/20 sm", "changed 710 to 791 8/26020 sm"]
			},
			{ "label": "createdDate",
				"fields": ["264", "260"],
				"selection": "first",
				"subfields": ["c"],
				"format": "text"
			},
			{ "label": "workType",
				"fields": ["leader"],
				"positions": [6],
				"extraProcessing": "lookup_work_type",
				"format": "text"
			},
			{ "label": "subjects",
				"fields": ["600", "689"],
				"selection": "range",
				"skipFields": ["655", "690"],
				"subfields": ["a", "b", "c", "d", "e", "f", "g", "i", "j", "k", "l", "m", "n", "o", "p", "q", "r", "s", "t", "u", "v", "w", "x", "y", "z"],
				"subfieldSeparator": " -- ",
				"specialSubfields": ["0"],
				"extraProcessing": "format_subjects",
				"format": "array",
				"note": "these subfields are defined here: https://www.loc.gov/marc/bibliographic/bd650.html and https://www.loc.gov/marc/bibliographic/bd6xx.html"
			},
			{ "label": "copyrightStatus",
				"fields": ["542"],
				"subfields": ["l"],
				"format": "text"
			},
			{ "label": "copyrightUrl",
				"fields": ["542"],
				"subfields": ["u"],
				"format": "text"
			},
			{ "label": "copyrightStatement",
				"fields": ["542"],
				"subfields": ["f"],
				"format": "text"
			},
			{
				"label": "digitalAccess",
				"note": "This must run after copyrightStatus is added above.",
				"externalProcess": "define_digital_access",
				"passLabels": [{"copyrightStatus": "copyrightStatus"}],
				"optional": true,
				"format": "text"
			},
			{ "label": "linkToSource",
				"fields": ["001"],
				"format": "text",
				"extraProcessing": "link_to_source"
			},
			{"label": "publisher",
				"otherNodes": "publisher",
				"format": "node"
			},
			{ "label": "format",
				"fields": ["300"],
				"subfields": ["a"],
				"format": "text"
			},
			{ "label": "languages",
				"fields": ["008"],
				"positions": [35, 36, 37],
				"format": "array"
			},
			{ "label": "dimensions",
				"fields": ["300"],
				"subfields": ["c"],
				"format": "text"
			},
			{ "label": "dedication",
				"fields": ["561"],
				"subfields": ["a"],
				"format": "text"
			},
			{ "label": "description",
				"fields": ["520"],
				"subfields": ["a", "b"],
				"format": "text"
			},
			{ "!label": "filePath",
				"!fields": ["956"],
				"!subfields": ["u"],
				"!verifySubfieldsMatch": {"subfield": "x", "value": "MARBLE"},
				"!format": "text"
			},
			{
				"label": "items",
				"otherNodes": "items",
				"optional": true,
				"seedNodes": [{"collectionId": "collectionId"}, {"parentId": "id"}, {"sourceSystem": "sourceSystem"}, {"repository": "repository"},
					{"apiVersion": "apiVersion"}, {"fileCreatedDate": "fileCreatedDate"}],
				"format": "array"
			},
			{
				"label": "level",
				"note": "This must run after items are added above, since children define level.",
				"externalProcess": "define_level",
				"passLabels": [{"items": "items"}],
				"format": "text"
			}
		]
	},
	"publisher" : {
		"FieldsToExtract": [
			{ "label": "publisherName",
				"fields": ["264", "260"],
				"selection": "first",
				"subfields": ["b"],
				"format": "text"
			},
			{ "label": "publisherLocation",
				"fields": ["264", "260"],
				"selection": "first",
				"subfields": ["a"],
				"format": "text"
			}
		],
	"note": "added 5/15/20 sm"
	},
	"items": {
		"FieldsToExtract": [
			{ "label": "level",
				"constant": "file",
				"format": "text"
			},
			{ "label": "sequence",
				"constant": 1,
				"format": "text"
			},
			{ "label": "thumbnail",
				"constant": true,
				"format": "text"
			},
			{ "label": "filePath",
				"fields": ["956"],
				"subfields": ["u"],
				"verifySubfieldsMatch": {"subfield": "x", "value": "MARBLE"},
				"format": "text"
			},
			{
				"label": "id",
				"externalProcess": "file_name_from_filePath",
				"passLabels": [{"filename": "filePath"}],
				"format": "text"
			},
			{
				"label": "description",
				"externalProcess": "file_name_from_filePath",
				"passLabels": [{"filename": "filePath"}],
				"format": "text"
			}
		]
	}
}
